{
  "mount": {
    "_output": "/",
    "src": "/build"
  },
  "devOptions": {
    "hmrDelay": 100
  },
  "optimize": {
    "bundle": true,
    "minify": true,
    "target": "es2020"
  },
  "plugins": [
    [
      "@snowpack/plugin-run-script",
      { "cmd": "eleventy", "watch": "$1 --watch" }
    ]
  ]
}
